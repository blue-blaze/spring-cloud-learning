    <modelVersion>4.0.0</modelVersion>

    <artifactId>spring-cloud-configclient</artifactId>
    <packaging>jar</packaging>

    <name>spring-cloud-configclient</name>
    <url>http://maven.apache.org</url>

    <properties>
